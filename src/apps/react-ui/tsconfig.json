{
  "compilerOptions": {
    "target": "es2016",
    "jsx": "preserve",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "src",
    "paths": {
      "@controllers/*": [
        "../../../controllers/*"
      ],
      "@data_model/*": [
        "../../../data_model/*"
      ]
    }
  }
}